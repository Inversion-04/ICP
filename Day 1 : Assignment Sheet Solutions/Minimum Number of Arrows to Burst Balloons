class Solution {
    public int findMinArrowShots(int[][] points) {

       
       Arrays.sort(points,(a,b)->Integer.compare(a[1],b[1]));

        int i = 1;
        int ans = 1;
        int end = points[0][1];
        while (i <points.length) {
            int x3 = points[i][0];

            if (x3 > end) {
                ans += 1;
                end = points[i][1];
            } 
            i++;
        }
        return ans;
    }
}
